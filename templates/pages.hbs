<html>
    <head>
        <title>{{title}}</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">

       <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/prismjs@1.29.0/themes/prism.min.css"> 
        {{#if externalCssIncludes}}
        <style>
            {{#each externalCssIncludes}}
            @import url('{{{this}}}');
            {{/each}}
        </style>    
        {{/if}}        
        {{#each headIncludes}}
        <link rel="stylesheet" href="{{this}}">
        {{/each}}
    </head>
    <body>
         <div style="width: 100%; min-height: 100px; background: linear-gradient(160deg, #007cf0, #00dfd8); display: flex; align-items: center; justify-content: center;">
 
<div class="container">    
    <h1 class="display-3 font-weight-light">{{title}}</h1>
    </div>
</div>

         </div>
         <div class="container mt-3">

        {{{content}}}        

</div>

    </body>
</html>